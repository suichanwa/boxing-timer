{
  "name": "boxing-timer",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "dev:android": "expo start --dev-client --android",
    "dev:ios": "expo start --dev-client --ios",
    "dev:clear": "expo start --dev-client --clear",
    "dev:android:clear": "expo start --dev-client --android --clear",
    "dev:ios:clear": "expo start --dev-client --ios --clear",
    "clean": "watchman watch-del-all && rm -rf node_modules && npm install && npx expo start --clear",
    "build:android": "eas build --platform android --profile development",
    "build:android:local": "eas build --platform android --profile development --local",
    "build:preview": "eas build --platform android --profile preview",
    "build:prod": "eas build --platform android --profile production",
    "prebuild": "expo prebuild",
    "install:dev": "eas build:run -p android"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.15",
    "@react-navigation/native-stack": "^7.2.1",
    "expo": "~52.0.37",
    "expo-av": "^15.0.2",
    "expo-dev-client": "~5.0.12",
    "expo-font": "~13.0.4",
    "expo-image": "~2.0.6",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3"
  },
  "private": true
}